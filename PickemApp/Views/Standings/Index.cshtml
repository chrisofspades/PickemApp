@model IEnumerable<PickemApp.Models.WeeklyPlayerPicks>

@{
    ViewBag.Title = "Standings";
}

<h1>@ViewBag.Year Overall Standings</h1>

<table class="standings">
@foreach (var pp in Model)
{
    <tr>
        <th scope="row">@pp.PlayerName</th>
        <td>@pp.CorrectPicks</td>
    </tr>
}
</table>
